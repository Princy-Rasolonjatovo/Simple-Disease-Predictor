<Window x:Class="DiseasePredictor.SearchResultWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DiseasePredictor"
        mc:Ignorable="d"
        Title="Prediction Result" Height="250" Width="350"
        ResizeMode="NoResize"
        >
    <Grid>
        <Label Content="You may have the following diseases : &#xD;&#xA;" HorizontalAlignment="Center" Margin="0,10,0,0" VerticalAlignment="Top" Width="330" Height="28" FontWeight="Normal"/>
        <ListBox x:Name="listViewResult" Margin="10,50,10,87" Background="#FFE6E6E6"/>
        <TextBlock HorizontalAlignment="Center" Margin="10,159,0,0" TextWrapping="Wrap" Text=" Always consult your doctor or other qualified healthcare provider with any questions you may have regarding a medical condition, procedure, or treatment, whether it is a prescription medication, over-the-counter drug, vitamin, supplement, or herbal alternative" VerticalAlignment="Top" Height="65" Width="324" FontSize="9" FontWeight="Bold"/>
        <!--<ListView d:ItemsSource="{d:SampleData ItemCount=5}" Margin="10,55,10,10">-->

    </Grid>
</Window>
